{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "Jwt": {
    "Issuer": "Plainly.Api",
    "Audience": "Plainly.Frontend",
    "ExpiresInMinutes": 60,
    "PublicKey": "[PUBLIC_KEY]",
    "PrivateKey": "[PRIVATE_KEY]"
  },
  "ConnectionStrings": {
    "DefaultConnection": "[CONNECTION_STRING]",
    "LoggingConnection": "Data Source=Logs/logs.db"
  },
  "AllowedHosts": "*"
}
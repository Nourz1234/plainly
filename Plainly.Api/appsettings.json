{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Jwt": {
    "Issuer": "Plainly.Api",
    "Audience": "Plainly.Frontend",
    "ExpiresInMinutes": 60,
    "PublicKey": "[PUBLIC_KEY]",
    "PrivateKey": "[PRIVATE_KEY]"
  },
  "ConnectionStrings": {
    "DefaultConnection": "[CONNECTION_STRING]"
  },
  "AllowedHosts": "*"
}
{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    }
  },
  "DbLogging": {
    "Enabled": true,
    "ConnectionString": "Data Source=Logs/logs.db"
  },
  "Jwt": {
    "Issuer": "Plainly.Api",
    "Audience": "Plainly.Frontend",
    "ExpiresInMinutes": 60,
    "KeyId": "[KEY_ID]",
    "PrivateKey": "[PRIVATE_KEY]",
    "PublicKey": "[PUBLIC_KEY]"
  },
  "ConnectionStrings": {
    "DefaultConnection": "[CONNECTION_STRING]"
  },
  "AllowedHosts": "*"
}